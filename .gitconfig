[user]
  name = John Klein
  email = johnmbklein@gmail.com
[core]
  editor = code --wait
  excludesfile = /Users/john/.gitignore_global
  autocrlf = input
[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
  ui = true
[init]
  defaultBranch = main
[alias]
  c = commit
  ca = commit -a
  cm = commit -m
  cam = commit -am
  d = diff
  dc = diff --cached
  l = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[filter "lfs"]
  clean = git-lfs clean %f
  smudge = git-lfs smudge %f
  required = true
